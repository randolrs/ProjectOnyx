
<div class="home-forecast-form">

  <%= f.hidden_field :_destroy %>
  <%= link_to "+", '#', class: "remove_fields cancel-prediction" %>

  <h4 class="forecast-form-header">Earnings Per Share Forecast</h4>

  <span><%= f.text_field :company, placeholder: "Company", autofocus: false%></span><br>

  <span>

    <select class="article_league_select">

      <option value="" disabled selected>Select Quarter</option>
      <option value="Q1">Q1</option>
      <option value="Q2">Q2</option>
      <option value="Q3">Q3</option>
      <option value="Q4">Q4</option>

    </select>

  </span><br>

  <span>

    <select class="article_league_select">

      <option value="" disabled selected>Select Year</option>
      <option value="2016">2016</option>
      <option value="2017">2017</option>
      <option value="2018">2018</option>
      <option value="2019">2019</option>

    </select>

  </span><br>

  <div class="prediction-save-button-container">
    
    <a class ="prediction-save-button">Save</a>

  </div>

  <fieldset class="final-score-select" style="display:none">

  </fieldset>

</div>
