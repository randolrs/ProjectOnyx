
<div class="prediction-index-table-holder">

  <table class ="prediction-index-fold table table-hover">
    <thead>
    <tr>
      <th class="prediction-index-fold-th">Matchup</th>
      <th class="prediction-index-fold-th">Date</th>
      <th class="prediction-index-fold-th">Time (EST)</th>
      
    </tr>
    </thead>

          <% @predictions.each do |prediction| %>

            <tbody>
            
              <div>
                <tr data-link="<%=predictiongamesshow_path(@predictor.username,prediction.id)%>", class="prediction-table-row" href="/">
                  <td> <%= prediction.teamh %> at <%= prediction.teama %></td>
                  <td> <%= prediction.event_time.strftime("%D") %></td>
                  <td> <%= prediction.event_time.strftime("%l:%M %p") %></td>
                </tr>
              </div>

            </tbody>

          <% end %>

      </table>

</div>