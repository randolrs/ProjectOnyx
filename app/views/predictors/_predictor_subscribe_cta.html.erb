<% if user_signed_in?%>

  	<% unless current_user.subscribed(predictor.id) %>

  		<% if predictor.subscription_price > 0 %>
		
			<%= link_to "Go Premium", subscribe_confirm_path(predictor.username), {:class=>"subscribe_cta" }%>

		<%end%>

	<%else%>

		<a class="subscribe_cta">&#10004; Premium<%# number_to_currency(@predictor.sub_price).to_s %></a>

  	<%end%>

<%end%>

