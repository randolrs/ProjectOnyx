<%= render 'predictors/predictorcover' %>
<%= render 'predictors/predictorcovernav' %>

<div class="content-panel">

	<h1>Listing Subscribers</h1>

<table>
  <thead>
    <tr>
      <th>Username</th>
      <th>Subscribed On</th>
    </tr>
  </thead>

  <tbody>
    <% @subscribers.each do |subscriber| %>
      <tr>
        <td><%= subscriber.username %></td>
        <td><%= subscriber.username %></td>
      </tr>
    <% end %>
  </tbody>
</table>
	
</div>
